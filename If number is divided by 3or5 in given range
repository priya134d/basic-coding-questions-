#include <iostream>
using namespace std;

int main()
{
int n;
cin>>n;
for(int i=1; i<=n; i++){
    if(i%3==0 && i%5==0){
        cout<<i<<" priya"<<endl;
    }
    else if(i%5==0 && i%3!=0){
        cout<<i<<" rahul"<<endl;
    }
    else if(i%3==0 && i%5!=0){
        cout<<i<<" rohit"<<endl;
    }
    else
    cout<<i<<" parents"<<endl;
}
    return 0;
}
