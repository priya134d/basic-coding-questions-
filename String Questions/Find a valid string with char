#include <iostream>
#include <string.h>
using namespace std;

int main()
{
  string s1;
  cin>>s1;
  int size = s1.length();
  cout<<size<<endl;
  
  for(int i=0; i<s1.length(); i++){
      
     if(s1[i]>='a'&&s1[i]<='z'||s1[i]>='A'&&s1[i]<='Z'){
           cout<<"valid string";
           return 1;
     }
     else
           cout<<"invalid string";
     
  }
  
    return 0;
}
