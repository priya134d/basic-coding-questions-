#include <iostream>
using namespace std;

int main()
{
   int num;
   cin>>num;
   int originalNum= num;
   int rev =0;
   while(num!=0){
       int rem = num%10;
       rev= (rev*10)+ rem;
       num = num/10;
       
   }
   cout<<rev<<endl;
   if(rev==originalNum)
       cout<<"palindrom"<<endl;
       else
       cout<<"Not palindrom"<<endl;
       

    return 0;
}
