#include <iostream>
using namespace std;

int main()
{
  int a[3][4],i,j;
  
  for(i=0;i<3;i++){
      for(j=0;j<4;j++){
          cin>>a[i][j];
      }
  }
  
  cout<<"print the matrix"<<endl;
  
    for(i=0;i<3;i++){
      for(j=0;j<4;j++){
          cout<<a[i][j]<<" ";
      }
      cout<<endl;
  }
  
    cout<<"Transpose of the matrix"<<endl;
  
    for(i=0;i<4;i++){
      for(j=0;j<3;j++){
          cout<<a[j][i]<<" ";
      }
      cout<<endl;
  }

    return 0;
}
